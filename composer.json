{
    "name": "sokolnikov911/testing-tools-updated",
    "description": "Laravel-specific Testing Helpers and Assertions.",
    "keywords": ["laravel", "testing", "tools", "helpers", "assertions", "assertion-methods", "assertion-functions"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sokolnikov911/laravel-testing-tools-updated/issues",
        "source": "https://github.com/sokolnikov911/laravel-testing-tools-updated"
    },
    "authors": [
        {
            "name": "Dmitry Ivanov",
            "email": "dmitry.g.ivanov@gmail.com"
        },
        {
            "name": "Petro Sokolnykov",
            "email": "info@xyz.net.ua"
        }
    ],
    "require": {
        "php": "~8.1|~8.2",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0",
        "mockery/mockery": "^1.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminated\\Testing\\Tests\\": "tests/",
            "Illuminated\\Testing\\Tests\\App\\": "tests/fixture/app/",
            "Illuminated\\Testing\\Tests\\Database\\Factories\\": "tests/fixture/database/factories/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
